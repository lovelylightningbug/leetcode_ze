/**
 * Definition for singly-linked list.
 * struct ListNode {
 *     int val;
 *     ListNode *next;
 *     ListNode(int x) : val(x), next(NULL) {}
 * };
 */
 
 
// Solution 1
class Solution {
public:
    ListNode* deleteDuplicates(ListNode* head) {
        if head->next->val == head->val
            head->next = head->next->next;
        
        }
    
private:
  ListNode *head;

};
